<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.PreferencesController">
  <children>
      <FlowPane alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="640.0" vgap="10.0" VBox.vgrow="ALWAYS">
         <children>
            <Button mnemonicParsing="false" onAction="#btnApply" text="Apply">
               <FlowPane.margin>
                  <Insets right="20.0" />
               </FlowPane.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#btnCancel" text="Cancel" />
         </children>
         <VBox.margin>
            <Insets right="20.0" />
         </VBox.margin>
      </FlowPane>
  </children>
</VBox>
