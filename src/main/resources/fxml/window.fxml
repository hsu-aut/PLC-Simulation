<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox minHeight="750.0" minWidth="1050.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Controller">
	<children>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
         <items>
            <Button fx:id="btnStart" minWidth="50.0" mnemonicParsing="false" onAction="#btnStart_oA" prefWidth="50.0" text="Start">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#btnReset_oA" text="Reset">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
         </items>
      </ToolBar>
      <VBox VBox.vgrow="ALWAYS">
         <children>
      				<ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
               <content>
                  <Pane fx:id="pane" minHeight="450.0" minWidth="900.0" />
               </content>
            </ScrollPane>
            <TextArea fx:id="tAConsole" editable="false">
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </TextArea>
         </children>
      </VBox>
	</children>
</VBox>
